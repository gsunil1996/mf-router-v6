{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/app1/remoteEntry.js",
      "dest": "/remoteEntry.js"
    },
    {
      "src": "/app1/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/app1/*",
      "dest": "/app1/index.html"
    },
    {
      "src": "/app1",
      "dest": "/app1/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
